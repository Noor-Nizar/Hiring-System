is_Qualified_for_Security
    foreach
        gender(male)
    assert  QualifiedForsecurity()
    
        
is_Qualified_for_Service
    foreach
        gender(female)
    assert  QualifiedForservice()
    

is_Qualified_for_receptionest
    foreach
        non_tech_cert(Languages, $_any)
    assert  QualifiedForreceptionest()



is_Qualified_For_Business
    foreach
        business_degree($_any)
        business_experience($field,$years)
    assert QualifiedForBuisness($field,$years)
    
is_Qualified_For_Business_SL
    foreach
        no_degree()
        business_cert($_any, $_any)
        business_experience($field, $years > 2)
    assert QualifiedForBuisness($field, $years-2 )
    

is_Qualified_for_Sales
    foreach
        QualifiedForBuisness(Sales,$_any)
    assert SalesPerson(T)
    

is_Qualified_For_Accounting
    foreach
        QualifiedForBuisness(Accounting,$_any)
    assert Accountant(T)
    