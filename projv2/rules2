no_degree
    foreach
        tech_degree($_any)
        non_tech_degree($_any)
    assert noDegree()
    
is_Qualified_For_Tech_SL
    foreach
        no_degree()
        tech_cert($_any, $_any)
        tech_experience($field, $years > 2)
    assert QualifiedForTech($field, $years-2)
    
    
is_Qualified_For_Tech
    foreach
        tech_degree($_any)
        tech_experience($field,$years)
    assert QualifiedForTech($field,$years)
    

is_Qualified_for_SEIntern
    foreach
        QualifiedForTech(SE,$_any < 2)
        tech_cert(SE, $_any)
    assert SEIntern(T)
    
is_Qualified_For_SEJunior
    foreach
        QualifiedForTech(SE,$_any >= 2)
        QualifiedForTech(SE,$_any < 5)
        tech_cert(SE, $_any)
    assert SEJunior(T)
    

is_Qualified_for_SESenior
    foreach
        QualifiedForTech(SE,$_any >= 5)
        tech_cert(SE, $_any)
    assert SESenior(T)
       

is_Qualified_for_DSIntern
    foreach
        QualifiedForTech(DS,$_any < 2)
        tech_cert(DS, $_any)
    assert DSIntern(T)
    

is_Qualified_for_DSJunior
    foreach
        QualifiedForTech(DS,$_any >= 2)
        QualifiedForTech(DS,$_any < 5)
        tech_cert(DS, $_any)
    assert DSJunior(T)
    

is_Qualified_for_DSSenior
    foreach
        QualifiedForTech(DS,$_any >= 5)
        tech_cert(DS, $_any)
    assert DSSenior(T)
    
