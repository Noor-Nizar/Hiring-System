is_Qualified_for_Security
    use  QualifiedForsecurity()
    when
        gender(male)
        
is_Qualified_for_Service
    use  QualifiedForservice()
    when
        gender(female)

is_Qualified_for_receptionest
    use  QualifiedForreceptionest()
    when
        non_tech_cert(Languages, $_any)

is_Qualified_for_Sales
    use SalesPerson(T)
    when
        QualifiedForBuisness($_any )
        business_cert(Sales,$_any)
        business_experience(Sales,$_any > 2)

is_Qualified_For_Accounting
    use QualifiedForAccounting(Persons.Experience)
    when
        QualifiedForBuisness($_any)
        business_cert(Accouning,$_any)
        business_experience(Accouning,$_any)